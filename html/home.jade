      color: #848589;
      }
      body .proginn-user-list .user-info .desc-item a {
      color: #848589;
      }
      body .proginn-user-list .user-info .desc-item a:first-child {
      color: #848589;
      }
      body .proginn-user-list .user-info .desc-item a:hover {
      color: #2994f2 !important;
      text-decoration: underline;
      }
