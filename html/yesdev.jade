      /**
      * 作品点赞、取消点赞
      * @param wid
      */
      function worksplustoggle(wid) {
      //防止连续快速点击
      if (loading) return;
      loading = true;
      $.ajax({
      type: "POST",
      url: "/ajax/worksplustoggle",
      data: { wid: wid },
      dataType: "json",
      success: function (msg) {
      if (msg.status == 1) {
      let zan = $('#zan');
      let zan_txt = $('#zan_txt');
      let zan_count = $("#zan_count");
      let _addClass = msg.data.is_cancel ? "app_bg_919AA7" : "app__bg_308EFF";
      let _removeClass = msg.data.is_cancel ? "app__bg_308EFF" : "app_bg_919AA7";
      zan.removeClass(_removeClass);//蓝
      zan.addClass(_addClass);//灰
      zan_txt.text(msg.data.is_cancel ? "赞" : "已赞");
      let zan_c = parseInt(zan_count.text());
      if (msg.data.is_cancel) {
      zan_count.text(zan_c > 0 ? (zan_c - 1) : 0);
      } else {
      zan_count.text(zan_c + 1);
      }
      } else {
      alert(msg.info);
      }
      loading = false;
      }
      });
      }
      function commentAdd(wid) {
      //防止连续快速点击
      if (loading) return;
      loading = true;
      cnzz_data("资源下载", "资源详情+评论", "");
      let commentContent = $("#comment #commentContent").val();
      if (!commentContent) {
      alert("请填写评论内容");
      return;
      }
      $("#btnComment").text("正在提交");
      $.ajax({
      type: "POST",
      url: "/api/UserWorkReplies/add",
      data: { wid: wid, body: commentContent },
      dataType: "json",
      success: (msg) => {
      loading = false;
      if (msg.status == 1) {
      //评论成功，一切还原
      commentData = [];
      pageIndex = 1;
      loadComment();
      $("#comment #commentContent").val("");
      } else {
      alert(msg.info);
      }
      $("#btnComment").text("评论");
      }
      });
      }
