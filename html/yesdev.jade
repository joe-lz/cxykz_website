      let commentData = [];
      let pageIndex = 1;
      let loading = false;
      function loadComment() {
      if (loading) return;
      loading = true;
      let homeUid = "448145";
      let loginUid = "675016";
      if (homeUid != loginUid) {
      $.ajax({
      type: "POST",
      url: "/api/UserWorkReplies/getList",
      data: {
      wid: '1423814',
      page: pageIndex,
      size: 10
      },
      dataType: "json",
      success: res => {
      if (res.status) {
      try {
      commentData = commentData.concat(res.data.list);
      $('#comment_totle').text(res.data.total + "评论");
      let html = commentHtmlBuild(commentData, res.data.total);
      $('#comment_list').html(html);
      } catch (e) {
      } finally {
      loading = false;
      pageIndex++;
      }
      }
      }, fail: (err) => {
      loading = false;
      pageIndex++;
      }
      });
      }
      }
      function commentHtmlBuild(commentDatas, total) {
      let html = "";
      commentDatas.forEach(item => {
      html += "<div class='app__f_c app__mt_10 app__mb_10'>" +
      "<div class='app__f_r app__al_r'>";
      //头像
      if (item.user_info.icon_url) {
      html += "<a href='" + item.user_info.developerHomePageURL + "'><img class='app__img_md app_br_50_P' src='" + item.user_info.icon_url + "'/></a>";
      } else {
      html += "<a href='" + item.user_info.developerHomePageURL + "'><img class='app__img_md app_br_50_P' src='https://stacdn.proginn.com/image/usericon/7.png'/></a>"
      }
      // 列表体
      html += "<div class='app__f_c app_f_g_1'>" +
      "<div class='app__f_r app__h_40 app__al_c'>" +
      "<a class='app_f_g_1 app__ml_10 app__fs_14' href='" + item.user_info.developerHomePageURL + "'>" + item.user_info.nickname + "</a>" +
      "                                            <p class='app__fs_12 app__fc_999'>" + item.create_time_name + "</p>" +
      "                                        </div>" +
      "                                        <div class='app__ml_10 app__fs_14 app__word_b_a'>" + item.body_original + "</div>" +
      "                                    </div>" +
      "                                </div>" +
      "                            </div>";
      });
      //加载更多按钮
      if (commentDatas.length < total) {
      html += "<div class='app__f_c app__mt_10 app__mb_10 app__cursor_point' onclick='addMore()'>" +
      "<div class='app__f_r app__al_c app__jf_c app__h_40'>";
      html += "<div id='addmore' class='app__fs_14'>加载更多</div>";
      html += "</div></div>"
      }
      return html;
      }
